{
  "name": "github-actions-monitor-backend",
  "version": "1.0.0",
  "description": "Backend for GitHub Actions Runner Observability Tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "seed": "tsx prisma/seed.ts",
    "typeorm:generate": "ts-node-esm ./node_modules/typeorm/cli.js schema:log",
    "typeorm:migrate:run": "ts-node-esm ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migrate:generate": "ts-node-esm ./node_modules/typeorm/cli.js migration:generate -n"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.2",
    "@google/generative-ai": "^0.24.1",
    "@octokit/auth-app": "^8.1.1",
    "@octokit/rest": "^22.0.0",
    "@octokit/webhooks": "^14.1.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
